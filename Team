#include <iostream>
using namespace std;


int main()
{
    int p,v,t, q, count=0, att=0;

    cin>>q;

    while(q--)
    {
        cin>>p>>v>>t;

        if(p==1)
        count++;
        if(v==1)
        count++;
        if(t==1)
        count++;

        if(count>=2)
        att++;   

        count = 0;

    }

    cout<<att;
return 0;
}
